                                        <div class="form-group">
                                            <div class="input-group">
                                                <span class="input-group-addon">
                                                    Hasta
                                                </span>
                                                <input type="text" name="hasta" id="txt_hasta" class="form-control input-sm" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-2">
                                        <div class="form-group">
                                            <div class="input-group">
                                                <span class="input-group-addon" style="font-size:11px;">
                                                    Estado
                                                </span>
                                                <select class="form-control input-sm" name="estado" id="cboestado">
                                                    <option value="">Seleccione</option>
                                                    <option value="1">Pendiente</option>
                                                    <option value="2">Completado</option>
                                                    <option value="3">Reenviado</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-2">
                                        <div class="form-group">
                                            <button class="btn btn-white btn-warning btn-sm btn-block btn-round btn_buscarFichas" data-id="1" data-rel="tooltip" title="Buscar">
                                                <i class="ace-icon glyphicon glyphicon-list-alt"></i> Buscar
                                            </button>
                                        </div>
                                    </div>
                                    <div class="col-md-2">
