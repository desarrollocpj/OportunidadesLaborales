
@{
    Layout = "~/Views/Shared/_LayoutIntranetPJAdmin.cshtml";
}

<div class="breadcrumbs ace-save-state" id="breadcrumbs">
    <ul class="breadcrumb">
        <li>
            <i class="ace-icon fa fa-home home-icon"></i>
            <a href="#">Inicio</a>
        </li>
        <li class="active">Panel Fichas Subida Archivo Excel</li>
    </ul><!-- /.breadcrumb -->
</div>

<div class="page-content">
    <div class="row">
        <div class="col-md-12">
            <div class="tabbable" style="margin-left:10px;margin-top:10px;">
                <ul class="nav nav-tabs" id="myTab3">
                    <li class="active">
                        <a data-toggle="tab" href="#home3">
                            <i class="orange ace-icon fa fa-tachometer bigger-110"></i>
                            Subida de Excel
                        </a>
                    </li>
                </ul>

                <div class="tab-content">
                    <div id="home3" class="tab-pane in active">
                        <div class="row" style="margin-bottom:10px;">
                            <div class="col-md-12 col-sm-12 col-xs-12">
                                <button class="btn btn-white btn-danger btn-block btn-round btn_descargar_modelo" data-rel="tooltip" title="Descargar">
                                    <i class="ace-icon fa fa-file"></i> Descargar Modelo
                                </button>
                            </div>
                        </div>
                        <div class="tabbable">
                            <div class="tab-content">

                                <div id="tabempleado" class="tab-pane in active">
                                    <div class="alert alert-warning alert-dismissible text-center"><span>Seleccione un archivo Excel</span></div>
                                    <div class="row" style="margin-bottom:10px;">
                                        <div class="col-md-2 col-sm-6 col-xs-6 pull-right">
                                            <button class="btn btn-white btn-success btn-sm btn-block btn-round btn_subir_excel" data-id="1" data-rel="tooltip" title="Enviar">
                                                <i class="ace-icon fa fa-file"></i> Subir Archivo
                                            </button>
                                        </div>
                                    </div>
                                    <div class="row">

                                        <form class="" id="form_excel">
                                            <div class="col-md-12 col-xs-12">
                                                <div class="form-group">
                                                    <div class="input-group">
                                                        <span class="input-group-addon input-group-sm">
                                                            Archivo
                                                        </span>
                                                        <input type="file" name="excel" id="excel" class="form-control" />
                                                    </div>
                                                </div>
                                            </div>
                                        </form>

                                        <div class="col-md-12">
                                            <table id="fichasExcelListado" class="table table-condensed table-striped table-bordered table-hover datatable-fichasExcelListado">
                                                <tbody>
                                                    <tr>
                                                        <td colspan="6">
                                                            <div class="alert alert-warning alert-dismissible text-center"><span>Listado de Correos Personales</span></div>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>

                    </div>
                </div>
            </div>

        </div>
    </div>
</div><!-- /.page-content -->





@section js
{
    <script src="~/Content/viewjs/IntranetPJAdmin/IntranetPJExcel.js"></script>
}





