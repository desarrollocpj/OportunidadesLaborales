    <script>
        $("#btnCerrarSesion").click(function () {
            $.ajax({
                type: 'POST',
                url: basePath + "WebCorporativaAdmin/WebCorporativaAdminCerrarSesionLoginJson",
                success: function (response) {
                    var resp = response.respuesta;
                    var msj = response.mensaje;
                    if (resp) {
                        window.location.replace(basePath + "WebCorporativaAdmin/Login");
                    } else {
                        alert(msj);
                    }
                }
            });
        });
    </script>
